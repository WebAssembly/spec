  | BBX      hint(show BBBX) hint(macro "%")                      ;; \BBBX
  | BBY      hint(show BBBY) hint(macro "x%y%z")                  ;; \xBBBYyBBBYz
  | BBZ      hint(show BBBZ) hint(macro none)                     ;; \mathsf{bbbz}
