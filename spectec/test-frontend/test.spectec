;;
;; Parameterisation
;;

def $f(syntax X, X) : X
def $f(syntax X, x) = x

def $g(def $h(syntax X, X) : X, syntax Y, Y) : Y
def $g(def $h, syntax Y, y) = $h(Y, y)

def $x : nat
def $x = $($g(def $f, syntax nat, 1) + $g($f, nat, 2))



